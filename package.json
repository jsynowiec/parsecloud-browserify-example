{
  "name": "parsecloud-browserify-example",
  "version": "0.1.0",
  "description": "An example Parse Cloud Code project using Browserify",
  "author": "Jakub Synowiec <jakub@jakubsynowiec.info>",
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "postinstall": "npm run setup",
    "setup": "mkdir cloud",
    "clean": "rm -rf cloud",
    "build": "browserify src/main.js --exclude buffer --exclude express -o cloud/main.js",
    "deploy": "npm run build && parse deploy"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "eslint": "^1.10.3"
  }
}
